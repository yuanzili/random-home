    "eslint": "^7.8.1",
    "eslint-plugin-react-hooks": "^4.1.0",
    "file-loader": "2.0.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.5.0",
